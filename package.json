{
  "name": "optional-components",
  "version": "1.0.6",
  "description": "similar to Unity's RequireComponent but supports various ways to declare (sibling) components as optional",
  "main": "index.js",
  "config": {
    "scope": "beatthat"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/beatthat/optional-components.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/beatthat/optional-components/issues"
  },
  "homepage": "https://github.com/beatthat/optional-components",
  "files": [
    "index.js",
    "scripts",
    "src",
    "Runtime",
    "Samples"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "postinstall": "node scripts/postinstall.js",
    "preinstall:test": "rm -f *.tgz",
    "install:test": "npm pack && cp ${npm_package_name}-${npm_package_version}.tgz ${npm_package_name}-latest.tgz && cd test && npm run clean && npm install",
    "clean": "rm -f *.tgz package-lock.json && cd test && npm run clean",
    "template:update": "node scripts/template-update.js",
    "cp:test2src": "cd test && node scripts/cp-test2src.js",
    "overwrite:test2src": "cd test && node scripts/overwrite-test2src.js",
    "version": "git add -A",
    "postversion": "git push && git push --tags && npm run clean"
  },
  "dependencies": {
    "get-components-ext": "github:beatthat/get-components-ext#semver:^1.0.0",
    "path": "^0.12.7",
    "unity-npm-utils": "github:beatthat/unity-npm-utils#semver:^1.0.0"
  },
  "devDependencies": {}
}
